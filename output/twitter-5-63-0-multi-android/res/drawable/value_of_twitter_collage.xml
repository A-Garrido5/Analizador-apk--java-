<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <bitmap android:gravity="center|left|top" android:src="@drawable/value_of_twitter_collage_1" />
    </item>
    <item android:top="@dimen/value_of_twitter_collage_split_height">
        <bitmap android:gravity="center|left|top" android:src="@drawable/value_of_twitter_collage_2" />
    </item>
    <item android:left="@dimen/value_of_twitter_collage_split_width">
        <bitmap android:gravity="center|left|top" android:src="@drawable/value_of_twitter_collage_1" />
    </item>
    <item android:left="@dimen/value_of_twitter_collage_split_width" android:top="@dimen/value_of_twitter_collage_split_height">
        <bitmap android:gravity="center|left|top" android:src="@drawable/value_of_twitter_collage_2" />
    </item>
</layer-list>